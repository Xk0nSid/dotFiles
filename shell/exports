export RLS_ROOT=${HOME}/git/rust/rls

# OCaml Opam
eval OPAM_SWITCH_PREFIX='/home/xk0nsid/.opam/default'; export OPAM_SWITCH_PREFIX;
CAML_LD_LIBRARY_PATH='/home/xk0nsid/.opam/default/lib/stublibs:Updated by package ocaml'; export CAML_LD_LIBRARY_PATH;
OCAML_TOPLEVEL_PATH='/home/xk0nsid/.opam/default/lib/toplevel'; export OCAML_TOPLEVEL_PATH;
MANPATH=':/home/xk0nsid/.opam/default/man'; export MANPATH;
