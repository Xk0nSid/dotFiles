export TERM="tmux-256color"

export EDITOR="vim"

export LC_ALL="en_US.UTF-8"

export PATH="$PATH:$HOME/bin"

export PATH="$HOME/.cargo/bin:$PATH"

export GOPATH=$HOME/workspace/go

export PATH="$PATH:$GOPATH/bin"
export PATH="$PATH:$HOME/go/bin"

export JIRA_URL="http://jira.shopclues.com"

export PATH="$HOME/.npm-global/bin:$PATH"

export PATH="$HOME/.local/bin:$PATH"

export PATH="$HOME/dotFiles/shell/scripts:$PATH"

# Rust Language Server
export DYLD_LIBRARY_PATH=${HOME}/.rustup/toolchains/stable-x86_64-unknown-linux-gnu/lib
export RLS_ROOT=${HOME}/git/rust/rls

# OCaml Opam
eval OPAM_SWITCH_PREFIX='/home/xk0nsid/.opam/default'; export OPAM_SWITCH_PREFIX;
CAML_LD_LIBRARY_PATH='/home/xk0nsid/.opam/default/lib/stublibs:Updated by package ocaml'; export CAML_LD_LIBRARY_PATH;
OCAML_TOPLEVEL_PATH='/home/xk0nsid/.opam/default/lib/toplevel'; export OCAML_TOPLEVEL_PATH;
MANPATH=':/home/xk0nsid/.opam/default/man'; export MANPATH;

export PATH="$PATH:$HOME/.opam/default/bin"

export PATH=$PATH:$HOME/.linkerd2/bin

export PATH=/home/xk0nsid/.nimble/bin:$PATH
